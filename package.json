{
  "name": "angular-architecture",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --code-coverage",
    "test:ci": "ng test --code-coverage --no-watch --no-progress --browsers=ChromeHeadlessCI",
    "format": "prettier --write './**/*.{ts,html,scss}'",
    "format:check": "prettier --check './**/*.{ts,html,scss}'",
    "stylelint": "stylelint './**/*.scss'",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "18.1.2",
    "@angular/common": "18.1.2",
    "@angular/compiler": "18.1.2",
    "@angular/core": "18.1.2",
    "@angular/forms": "18.1.2",
    "@angular/platform-browser": "18.1.2",
    "@angular/platform-browser-dynamic": "18.1.2",
    "@angular/router": "18.1.2",
    "angular-cli-ghpages": "1.0.5",
    "rxjs": "7.8.1",
    "tslib": "2.6.3",
    "zone.js": "0.14.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "18.1.2",
    "@angular/cli": "18.1.2",
    "@angular/compiler-cli": "18.1.2",
    "@testing-library/angular": "17.1.0",
    "@testing-library/dom": "^10.0.0",
    "@types/jasmine": "5.1.4",
    "angular-eslint": "18.1.0",
    "angular-in-memory-web-api": "0.17.0",
    "eslint": "^9.8.0",
    "jasmine-core": "5.2.0",
    "karma": "6.4.3",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "prettier": "3.2.5",
    "stylelint": "16.5.0",
    "stylelint-config-standard": "36.0.0",
    "typescript": "5.5.4",
    "typescript-eslint": "8.0.0-alpha.38"
  }
}
